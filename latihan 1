txt = "Hello World"

# 1. Hitung jumlah karakter
print("Jumlah karakter:", len(txt))

# 2. Ambil karakter terakhir
print("Karakter terakhir:", txt[-1])

# 3. Ambil karakter index ke-2 sampai ke-4
print("Index 2 sampai 4:", txt[2:5])

# 4. Hilangkan spasi
print("Tanpa spasi:", txt.replace(" ", ""))

# 5. Ubah menjadi huruf besar
print("Huruf besar:", txt.upper())

# 6. Ubah menjadi huruf kecil
print("Huruf kecil:", txt.lower())

# 7. Ganti karakter H dengan J
print("Ganti H dengan J:", txt.replace("H", "J"))
